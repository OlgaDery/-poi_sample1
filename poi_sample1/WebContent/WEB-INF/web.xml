<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
 <display-name>poi_sample</display-name>
 <filter>
  <filter-name>Pois_for_visualization</filter-name>
  <filter-class>filters.Pois_for_visualization</filter-class>
 </filter>
 <filter-mapping>
  <filter-name>Pois_for_visualization</filter-name>
  <url-pattern>/map/ajax</url-pattern>
 </filter-mapping>
 <servlet>
  <servlet-name>Servl_1</servlet-name>
  <servlet-class>servlets.Servl_1</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>Servl_1</servlet-name>
  <url-pattern>/map/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Servl_1</servlet-name>
  <url-pattern>/user/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Servl_1</servlet-name>
  <url-pattern>/user</url-pattern>
 </servlet-mapping>
 <session-config>
  <session-timeout>60</session-timeout>
 </session-config>
 <welcome-file-list>
  <welcome-file>/index.jsp</welcome-file>
 </welcome-file-list>
 <security-constraint>
  <display-name>Poi_app-user</display-name>
  <web-resource-collection>
   <web-resource-name>User-Pages</web-resource-name>
   <url-pattern>/user/*</url-pattern>
   <http-method>GET</http-method>
   <http-method>POST</http-method>
  </web-resource-collection>
  <auth-constraint>
   <description>UserRoles</description>
   <role-name>admin</role-name>
   <role-name>contrib</role-name>
   <role-name>user</role-name>
  </auth-constraint>
  <user-data-constraint>
   <transport-guarantee>NONE</transport-guarantee>
  </user-data-constraint>
 </security-constraint>
 <login-config>
  <auth-method>FORM</auth-method>
  <realm-name>poi_app</realm-name>
  <form-login-config>
   <form-login-page>/login.jsp</form-login-page>
   <form-error-page>/login-error.jsp</form-error-page>
  </form-login-config>
 </login-config>
 <security-role>
  <role-name>admin</role-name>
 </security-role>
 <security-role>
  <role-name>contrib</role-name>
 </security-role>
 <security-role>
  <role-name>user</role-name>
 </security-role>
 <security-role>
  <role-name>guest</role-name>
 </security-role>
</web-app>
